    "import pandas as pd #data manipulation lib\n",
    "import matplotlib.pyplot as plt #data visualization lib\n",
    "import seaborn as sns #data visualisation lib\n",
    "microsoft_df=pd.read_csv('Microsoft_Data.csv')\n",
    "microsoft_df.columns\n",
    "microsoft_df.isnull().sum()\n",
    "\n",
    "#microsoft_df['HasDetections'].value_counts()\n",
    "missing_values_df=pd.DataFrame({'col_name':microsoft_df.columns,\n",
    "                               'missing_value_count':microsoft_df.isnull().sum(),\n",
    "                               'missing_value_percentage':microsoft_df.isnull().sum()/microsoft_df.shape[0]*100})\n",
    "missing_values_df.sort_values(by='missing_value_percentage',ascending=False)\n",
    "\n",
    "column_to_drop=list(missing_values_df[missing_values_df['missing_value_percentage']>50]['col_name'])\n",
    "microsoft_df.drop(columns=column_to_drop,inplace=True)\n",
    "\n",
    "\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "int_col=[col for col in microsoft_df.columns\n",
    "         if microsoft_df[col].dtype=='int64' or microsoft_df[col].dtype=='float64']\n",
    "for col in int_col:\n",
    "    \n",
    "     microsoft_df[col].fillna(microsoft_df[col].mean(),inplace=True)\n",
    "microsoft_df.isnull() ##to check if column is filled\n",
    "obj_col=[col for col in microsoft_df.columns\n",
    "          if microsoft_df[col].dtype=='object']\n",
    "for col in obj_col:\n",
    "      microsoft_df[col].fillna(microsoft_df[col].mode(),inplace=True)\n",
    "    \n"
   ]
